  <sessions>
    <session name="http-example" probability="100" type="ts_http">
      <setdynvars sourcetype="random_number" start="1000"
		    end="99999999">
        <var name="random_username" />
      </setdynvars>

      <!-- Have the new users sign up  -->
      <request>
        <dyn_variable name="authenticity_token" xpath="/html/head/meta[@name='csrf-token']/@content"></dyn_variable>
        <http url='/users/sign_up' version='1.1' method='GET'>
        </http>
      </request>
			<setdynvars sourcetype="eval"
		    code="fun({Pid,DynVars})->{ok,[Val]}=ts_dynvars:lookup(authenticity_token,DynVars), http_uri:encode(Val) end.">
	      <var name="encoded_authenticity_token" />
      </setdynvars>
      <request subst="true">
        <http url='/users' 
          version='1.1'  
          contents='utf8=%E2%9C%93&amp;authenticity_token=%%_encoded_authenticity_token%%&amp;user%5Bfirst_name%5D=&amp;user%5Blast_name%5D=&amp;user%5Busername%5D=%%_random_username%%&amp;user%5Bphone%5D=&amp;user%5Bemail%5D=%%_random_username%%%40gmail.com&amp;user%5Bpassword%5D=123456&amp;user%5Bpassword_confirmation%5D=&amp;commit=Create' 
